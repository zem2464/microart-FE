# Frontend Deployment Configuration
# Copy this to .env before running deploy-s3.sh

# AWS S3 Bucket
S3_BUCKET=microart-frontend-prod

# CloudFront Distribution ID (optional - for cache invalidation)
CLOUDFRONT_DISTRIBUTION_ID=E1234ABCDEF567

# React App Environment Variables
REACT_APP_GRAPHQL_URL=https://api.imagecare.in/graphql
REACT_APP_GRAPHQL_WS_URL=wss://api.imagecare.in/graphql
REACT_APP_API_URL=https://api.imagecare.in
REACT_APP_VAPID_PUBLIC_KEY=your-vapid-public-key-here
